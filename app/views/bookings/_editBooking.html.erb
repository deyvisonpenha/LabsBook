<div class="col-md-6" id="editBooking">
	<div class="panel panel-primary"> 
		<div class="panel-heading"> 
			<h3 class="panel-title">Editar Agendamento</h3> 
		</div> 
		<div class="panel-body"> 

			<p>
				<% if @bookings == nil%> 
				<h1>Nao possui agendamentos</h1>
				<% else%>
				<ul>
					<% @bookings.each do |user| %>
					<li><%= user.disciplina %></li>
					<% end %>
				</ul>
				<% end %>
			</p>

			

		</div> 
	</div>
</div>
